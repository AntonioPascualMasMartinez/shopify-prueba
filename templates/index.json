    "custom_liquid_iWLLgc": {
      "type": "custom-liquid",
      "name": "t:names.custom_liquid",
      "settings": {
        "custom_liquid": "<style>\n  .custom-info-grid {\n    display: grid;\n    grid-template-columns: repeat(3, 1fr);\n    gap: 25px;\n    max-width: 1200px;\n    margin: 40px auto;\n    padding: 0 20px;\n  }\n  \n  .custom-info-card {\n    background-color: #f4f6fb; /* Fondo azul/gris muy claro */\n    border-radius: 20px;\n    overflow: hidden;\n    display: flex;\n    flex-direction: column;\n    transition: transform 0.3s ease, box-shadow 0.3s ease;\n  }\n  \n  /* Opcional: un pequeño efecto al pasar el ratón por toda la tarjeta */\n  .custom-info-card:hover {\n    transform: translateY(-5px);\n    box-shadow: 0 10px 20px rgba(0,0,0,0.05);\n  }\n  \n  .custom-info-img {\n    width: 100%;\n    height: 220px;\n    object-fit: cover;\n  }\n  \n  .custom-info-content {\n    padding: 25px;\n    display: flex;\n    flex-direction: column;\n    flex-grow: 1;\n    justify-content: space-between;\n  }\n  \n  .custom-info-title {\n    color: #2b3a82;\n    font-size: 18px;\n    font-weight: 800;\n    margin: 0 0 25px 0;\n    line-height: 1.3;\n  }\n  \n  /* Estilo único para todos los botones */\n  .custom-info-btn {\n    align-self: flex-start;\n    padding: 10px 24px;\n    border-radius: 50px;\n    font-size: 13px;\n    font-weight: 500;\n    text-transform: uppercase;\n    text-decoration: none;\n    display: inline-flex;\n    align-items: center;\n    gap: 10px;\n    transition: all 0.3s ease;\n    /* Estilos del borde azul */\n    background-color: transparent;\n    color: #2b3a82;\n    border: 1px solid #2b3a82;\n  }\n\n  /* Efecto hover para todos los botones */\n  .custom-info-btn:hover {\n    background-color: #2b3a82;\n    color: #ffffff;\n  }\n\n  /* Responsive */\n  @media (max-width: 900px) {\n    .custom-info-grid { grid-template-columns: repeat(2, 1fr); }\n  }\n  @media (max-width: 600px) {\n    .custom-info-grid { grid-template-columns: 1fr; }\n  }\n</style>\n\n<div class=\"custom-info-grid\">\n\n  <div class=\"custom-info-card\">\n    <img src=\"https://cdn.shopify.com/s/files/1/1017/7177/2234/files/DSC_6741_1.jpg?v=1771776655\" class=\"custom-info-img\" alt=\"Vender productos\">\n    <div class=\"custom-info-content\">\n      <h3 class=\"custom-info-title\">¿Quieres vender nuestros productos?</h3>\n      <a href=\"/pages/vender\" class=\"custom-info-btn\">Saber más &rarr;</a>\n    </div>\n  </div>\n\n  <div class=\"custom-info-card\">\n    <img src=\"https://cdn.shopify.com/s/files/1/1017/7177/2234/files/shipping_1.jpg?v=1771777272\" class=\"custom-info-img\" alt=\"Envíos\">\n    <div class=\"custom-info-content\">\n      <h3 class=\"custom-info-title\">Realizamos envíos rápidos y seguros a todo el mundo</h3>\n      <a href=\"/pages/envios\" class=\"custom-info-btn\">Saber más &rarr;</a>\n    </div>\n  </div>\n\n  <div class=\"custom-info-card\">\n    <img src=\"https://cdn.shopify.com/s/files/1/1017/7177/2234/files/Blog_1.jpg?v=1771777272\" class=\"custom-info-img\" alt=\"Comunidad\">\n    <div class=\"custom-info-content\">\n      <h3 class=\"custom-info-title\">La comunidad para los amantes del modelismo</h3>\n      <a href=\"/pages/comunidad\" class=\"custom-info-btn\">Saber más &rarr;</a>\n    </div>\n  </div>\n\n  <div class=\"custom-info-card\">\n    <img src=\"https://cdn.shopify.com/s/files/1/1017/7177/2234/files/contacta.jpg?v=1771777335\" class=\"custom-info-img\" alt=\"Dudas\">\n    <div class=\"custom-info-content\">\n      <h3 class=\"custom-info-title\">¿Tienes alguna duda? Contacta con nosotros</h3>\n      <a href=\"/pages/contacto\" class=\"custom-info-btn\">Contactar &rarr;</a>\n    </div>\n  </div>\n\n  <div class=\"custom-info-card\">\n    <img src=\"https://cdn.shopify.com/s/files/1/1017/7177/2234/files/Solicitud_de_instrucciones.jpg?v=1771777336\" class=\"custom-info-img\" alt=\"Instrucciones\">\n    <div class=\"custom-info-content\">\n      <h3 class=\"custom-info-title\">¿Necesitas instrucciones para un kit?</h3>\n      <a href=\"/pages/instrucciones\" class=\"custom-info-btn\">Pídelas aquí &rarr;</a>\n    </div>\n  </div>\n\n  <div class=\"custom-info-card\">\n    <img src=\"https://cdn.shopify.com/s/files/1/1017/7177/2234/files/Solicitud_de_piezas.jpg?v=1771777336\" class=\"custom-info-img\" alt=\"Reposición\">\n    <div class=\"custom-info-content\">\n      <h3 class=\"custom-info-title\">Reposición de piezas</h3>\n      <a href=\"/pages/piezas\" class=\"custom-info-btn\">Pídelas aquí &rarr;</a>\n    </div>\n  </div>\n\n</div>",
        "color_scheme": "",
        "section_width": "page-width",
        "padding-block-start": 0,
        "padding-block-end": 0
      }
