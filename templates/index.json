    "custom_liquid_nmkLGF": {
      "type": "custom-liquid",
      "name": "t:names.custom_liquid",
      "settings": {
        "custom_liquid": "<style>\n  .custom-category-grid {\n    display: grid;\n    grid-template-columns: repeat(4, 1fr);\n    gap: 20px;\n    max-width: 1200px;\n    margin: 40px auto;\n    padding: 0 20px;\n  }\n  \n  .custom-cat-card {\n    background-color: #f4f6ff; \n    border-radius: 20px;\n    padding: 30px 20px;\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n    text-align: center;\n    text-decoration: none;\n    transition: transform 0.3s ease, box-shadow 0.3s ease;\n  }\n  \n  .custom-cat-card:hover {\n    transform: translateY(-5px); /* Pequeño salto al pasar el ratón */\n    box-shadow: 0 10px 20px rgba(0,0,0,0.05);\n  }\n  \n  .custom-cat-title {\n    color: #2b3a82; /* Azul oscuro corporativo */\n    font-size: 29px;\n    font-weight: 600;\n    text-transform: uppercase;\n    line-height: 1.2;\n    margin: 0 0 20px 0;\n    min-height: 48px;\n    display: flex;\n    align-items: center;\n  }\n  \n  .custom-cat-img {\n    width: 100%;\n    object-fit: contain; /* Evita que la imagen se deforme */\n    margin-bottom: 25px;\n  }\n  \n  .custom-cat-btn {\n    background-color: #ffffff;\n    color: #2b3a82;\n    border-radius: 50px;\n    padding: 12px 0;\n    width: 90%;\n    max-width: 200px;\n    font-weight: 600;\n    font-size: 14px;\n    text-transform: uppercase;\n    transition: all 0.3s;\n  }\n  \n  .custom-cat-card:hover .custom-cat-btn {\n    background-color: #2b3a82;\n    color: #ffffff; /* El botón se invierte al pasar el ratón */\n  }\n\n  /* Adaptación para tablets (2 columnas) y móviles (1 columna) */\n  @media (max-width: 900px) {\n    .custom-category-grid { grid-template-columns: repeat(2, 1fr); }\n  }\n  @media (max-width: 500px) {\n    .custom-category-grid { grid-template-columns: 1fr; }\n  }\n</style>\n\n<div class=\"custom-category-grid\">\n  <a href=\"/collections/maquetas-de-barcos\" class=\"custom-cat-card\">\n    <h3 class=\"custom-cat-title\">Maquetas de<br>barcos</h3>\n    <img src=\"https://cdn.shopify.com/s/files/1/1017/7177/2234/files/SUPPLY_1.png?v=1771765664\" class=\"custom-cat-img\" alt=\"Maquetas de barcos\">\n    <span class=\"custom-cat-btn\">Descubrir</span>\n  </a>\n\n  <a href=\"/collections/maquetas-de-aviones\" class=\"custom-cat-card\">\n    <h3 class=\"custom-cat-title\">Maquetas de<br>aviones</h3>\n    <img src=\"https://cdn.shopify.com/s/files/1/1017/7177/2234/files/maquetas-de-aviones.png?v=1771765664\" class=\"custom-cat-img\" alt=\"Maquetas de aviones\">\n    <span class=\"custom-cat-btn\">Descubrir</span>\n  </a>\n\n  <a href=\"/collections/herramientas\" class=\"custom-cat-card\">\n    <h3 class=\"custom-cat-title\">Herramientas</h3>\n    <img src=\"https://cdn.shopify.com/s/files/1/1017/7177/2234/files/DSC7550_1.png?v=1771765664\" class=\"custom-cat-img\" alt=\"Herramientas\">\n    <span class=\"custom-cat-btn\">Descubrir</span>\n  </a>\n\n  <a href=\"/collections/acabados\" class=\"custom-cat-card\">\n    <h3 class=\"custom-cat-title\">Acabados</h3>\n    <img src=\"https://cdn.shopify.com/s/files/1/1017/7177/2234/files/pinturas.png?v=1771765812\" class=\"custom-cat-img\" alt=\"Acabados\">\n    <span class=\"custom-cat-btn\">Descubrir</span>\n  </a>\n</div>",
        "color_scheme": "",
        "section_width": "page-width",
        "padding-block-start": 0,
        "padding-block-end": 0
      }
    },
    "custom_liquid_BQ86MQ": {
      "type": "custom-liquid",
      "name": "t:names.custom_liquid",
      "settings": {
        "custom_liquid": "<style>\n  .custom-social-section {\n    text-align: center;\n    padding: 40px 20px;\n    background-color: #ffffff;\n  }\n  \n  .custom-social-title {\n    color: #2b3a82; /* Azul corporativo */\n    font-size: 28px;\n    font-weight: 800;\n    text-transform: uppercase;\n    margin-bottom: 25px;\n    letter-spacing: 0.5px;\n  }\n  \n  .custom-social-icons {\n    display: flex;\n    justify-content: center;\n    gap: 10px;\n  }\n  \n  .custom-social-btn {\n    display: inline-flex;\n    align-items: center;\n    justify-content: center;\n    width: 45px;\n    height: 45px;\n    color: #ffffff;\n    text-decoration: none;\n    border-radius: 4px; /* Bordes ligeramente redondeados como en la foto */\n    transition: transform 0.3s ease;\n  }\n  \n  .custom-social-btn:hover {\n    transform: translateY(-3px);\n  }\n  \n  .btn-facebook { background-color: #3b5998; }\n  .btn-instagram { background-color: #262626; }\n  .btn-youtube { background-color: #c4302b; }\n  \n  .custom-social-btn svg {\n    width: 24px;\n    height: 24px;\n    fill: currentColor;\n  }\n</style>\n\n<div class=\"custom-social-section\">\n  <h2 class=\"custom-social-title\">Síguenos en nuestras redes sociales</h2>\n  \n  <div class=\"custom-social-icons\">\n    <a href=\"TU_LINK_DE_FACEBOOK\" target=\"_blank\" class=\"custom-social-btn btn-facebook\" aria-label=\"Facebook\">\n      <svg viewBox=\"0 0 24 24\"><path d=\"M18 2h-3a5 5 0 0 0-5 5v3H7v4h3v8h4v-8h3l1-4h-4V7a1 1 0 0 1 1-1h3z\"></path></svg>\n    </a>\n    \n    <a href=\"TU_LINK_DE_INSTAGRAM\" target=\"_blank\" class=\"custom-social-btn btn-instagram\" aria-label=\"Instagram\">\n      <svg viewBox=\"0 0 24 24\"><rect x=\"2\" y=\"2\" width=\"20\" height=\"20\" rx=\"5\" ry=\"5\"></rect><path d=\"M16 11.37A4 4 0 1 1 12.63 8 4 4 0 0 1 16 11.37z\"></path><line x1=\"17.5\" y1=\"6.5\" x2=\"17.51\" y2=\"6.5\"></line></svg>\n    </a>\n    \n    <a href=\"TU_LINK_DE_YOUTUBE\" target=\"_blank\" class=\"custom-social-btn btn-youtube\" aria-label=\"YouTube\">\n      <svg viewBox=\"0 0 24 24\"><path d=\"M22.54 6.42a2.78 2.78 0 0 0-1.94-2C18.88 4 12 4 12 4s-6.88 0-8.6.46a2.78 2.78 0 0 0-1.94 2A29 29 0 0 0 1 11.75a29 29 0 0 0 .46 5.33 2.78 2.78 0 0 0 1.94 2c1.72.46 8.6.46 8.6.46s6.88 0 8.6-.46a2.78 2.78 0 0 0 1.94-2 29 29 0 0 0 .46-5.33 29 29 0 0 0-.46-5.33z\"></path><polygon points=\"9.75 15.02 15.5 11.75 9.75 8.48 9.75 15.02\"></polygon></svg>\n    </a>\n  </div>\n</div>",
